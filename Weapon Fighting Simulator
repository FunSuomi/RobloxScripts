local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Weapon Fighting Simulator | FinSuomi Hub", "DarkTheme")

--| TABS |--

local Main = Window:NewTab("Weapon Fighting Sim")
local Player = Window:NewTab("Discord")



--| MOVEMENT SECTION (CLIENT TAB) |--

local MainSection = Main:NewSection("Scripts") 
local PlayerSection = Player:NewSection("https://discord.gg/QExYcq4CqQ")

MainSection:NewButton("Ultra Hub", "Working Hub!", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ZaRdoOx/Ultra-Hub/main/Main"))()
end)

MainSection:NewButton("RIP Script", "Working Hub!", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vfvo/scripts/main/weaponfightsim.lua"))() 
end)

PlayerSection:NewButton("OUR DISCORD", "JOIN OUR DISCORD SERVER", function()
    print("https://discord.gg/QExYcq4CqQ")
end)
