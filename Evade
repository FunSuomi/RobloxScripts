local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Jail Break | FinSuomi Hub", "DarkTheme")

--| TABS |--

local Main = Window:NewTab("Jail Break")
local Player = Window:NewTab("Discord")



--| MOVEMENT SECTION (CLIENT TAB) |--

local MainSection = Main:NewSection("Scripts") 
local PlayerSection = Player:NewSection("https://discord.gg/QExYcq4CqQ")

MainSection:NewButton("Hydra Hub #1", "BEST + NO KEYS", function()
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/Robobo2022/script/main/Main.lua"), true))()
end)

PlayerSection:NewButton("OUR DISCORD", "JOIN OUR DISCORD SERVER", function()
    toclipboard("https://discord.gg/QExYcq4CqQ")
end)
