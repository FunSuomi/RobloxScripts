local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Strongest BattleGrounds | FinSuomi Hub", "DarkTheme")

--| TABS |--

local Main = Window:NewTab("Strongest Battle..")
local Player = Window:NewTab("Discord")



--| MOVEMENT SECTION (CLIENT TAB) |--

local MainSection = Main:NewSection("Scripts") 
local PlayerSection = Player:NewSection("https://discord.gg/QExYcq4CqQ")

MainSection:NewButton("Nicuse Hub", "OP", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/6gbWyDrr"))()
end)

MainSection:NewButton("Xtrey10x Hub", "OP!", function()   
    loadstring(game:HttpGet("https://raw.githubusercontent.com/xtrey10x/xtrey10x-hub/main/saitama"))()
end)

PlayerSection:NewButton("OUR DISCORD", "JOIN OUR DISCORD SERVER", function()
    toclipboard("https://discord.gg/QExYcq4CqQ")
end)
