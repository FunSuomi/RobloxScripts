local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pls Donate  | FinSuomi Hub", "DarkTheme")

--| TABS |--

local Main = Window:NewTab("Pls Donate")
local Player = Window:NewTab("Discord")

--| MOVEMENT SECTION (CLIENT TAB) |--

local MainSection = Main:NewSection("Scripts")
local PlayerSection = Player:NewSection("https://discord.gg/QExYcq4CqQ")

MainSection:NewButton("Anti-Moon-Fors", "really good ngl", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/animooon"))()
end)

MainSection:NewButton("Fake Donate", "fake donate to others", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/master/gdvdg"))()
end)

PlayerSection:NewButton("OUR DISCORD", "JOIN OUR DISCORD SERVER", function()
    toclipboard("https://discord.gg/QExYcq4CqQ")
end)
