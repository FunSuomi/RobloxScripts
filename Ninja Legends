local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ninja Legends | FinSuomi Hub", "DarkTheme")

--| TABS |--

local Main = Window:NewTab("Ninja Legends")
local Player = Window:NewTab("Discord")



--| MOVEMENT SECTION (CLIENT TAB) |--

local MainSection = Main:NewSection("Scripts") 
local PlayerSection = Player:NewSection("https://discord.gg/QExYcq4CqQ")

MainSection:NewButton("No Name", "OP", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/zerpqe/script/main/NinjaLegends.lua"))()
end)

MainSection:NewButton("Strike Hub", "OP!", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/StormSKz12/StirkeHub1/main/Gameincluded"))()
end)

MainSection:NewButton("Reaper Hub", "op", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HydraVirgo/ninjalegendsfreegui/main/obfusc", true))()
end)

PlayerSection:NewButton("OUR DISCORD", "JOIN OUR DISCORD SERVER", function()
    print("https://discord.gg/QExYcq4CqQ")
end)
