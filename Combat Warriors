local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Combat Warriors | FinSuomi Hub", "DarkTheme")

--| TABS |--

local Main = Window:NewTab("Combat Warriors")
local Player = Window:NewTab("Discord")



--| MOVEMENT SECTION (CLIENT TAB) |--

local MainSection = Client:NewSection("Scripts") 
